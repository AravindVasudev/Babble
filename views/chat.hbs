<script src="https://cdn.socket.io/socket.io-1.2.0.js"></script>

<input type="hidden" id="userid" value="{{user.id}}">
<div class="app-container">
  <header class="app-header noselect">
    <div class="app-header-content">

      <div class="dropdown">
        <div class="menu-button dropdown-toggle"  data-toggle="dropdown">
          <div class="bar1"></div>
          <div class="bar2"></div>
          <div class="bar3"></div>
        </div>

        <ul class="dropdown-menu">
          <li>
            <input type="file" name="file" id="file" class="visuallyhidden" accept="image/*">
            <label for="file">UPLOAD PIC</label>
          </li>
          <li><a href="https://github.com/AravindVasudev/Babble.gq" target="_blank">FORK ME</a></li>
          <li><a href="/auth/logout">LOGOUT</a></li>
        </ul>
      </div>


      <h1>babble chat</h1>
    </div>
  </header>
  <div class="row no-gutter app-body">
    <main class="col-md-9"> <!-- <main> -->
      <div class="messages">
        <div id="messages">

          {{#each history}}
            {{#areEqual id ../user.id }}
              <div class="message-me">
                {{#if msg}}
                  <span class="msg">{{{msg}}}</span>
                {{/if}}
                {{#if image}}
                  <span class="image"><img src="{{{image}}}" class="img-responsive"></span>
                {{/if}}
                <span class="time noselect">{{time}}</span>
              </div>
            {{else}}
            <div class="message">
              <span class="username noselect">{{name}}</span>
              {{#if msg}}
                <span class="msg">{{{msg}}}</span>
              {{/if}}
              {{#if image}}
                <span class="image"><img src="{{{image}}}" class="img-responsive"></span>
              {{/if}}
              <span class="time noselect">{{time}}</span>
            </div>
            {{/areEqual}}
          {{/each}}
        </div>
      </div>
      <div class="sender noselect">
        <form>
          <input type="text" autocomplete="off" placeholder="Type a message" id="type-message">
          <button type="submit" class="send-button"><span class="glyphicon glyphicon-chevron-right"></span></button>
        </form>
      </div>
    </main>
